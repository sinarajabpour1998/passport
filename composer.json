{
    "name": "sra/passport",
    "description": "a minimal api version of laravel passport",
    "minimum-stability": "stable",
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/sinarajabpour1998/passport",
    "authors": [
        {
            "name": "Sina Rajabpour",
            "email": "sina.rajabpour@outlook.com"
        }
    ],
    "require": {
        "php": "^7.1.3|8.*",
        "laravel/framework": "7.*|8.*",
        "laravel/sanctum": "^2.11"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0|^7.0",
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "SRA\\Passport\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SRA\\Passport\\PassportServiceProvider"
            ]
        }
    }
}
